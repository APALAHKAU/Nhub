local Tab = Window:NewTab("‚öôÔ∏èSettings")

local Section = Tab:NewSection("UI Settings")
Section:NewKeybind("Toggle UI", "Toggle the UI", Enum.KeyCode.RightControl, function()
    Library:ToggleUI()
end)
Section:NewButton("Toggle Ui For Mobile", "Toggle Ui", function()
    local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ScreenGui"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

local Toggle = Instance.new("TextButton")
Toggle.Name = "Toggle"
Toggle.Parent = ScreenGui
Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Toggle.Position = UDim2.new(0, 0, 0.454706937, 0)
Toggle.Size = UDim2.new(0, 90, 0, 38)
Toggle.Font = Enum.Font.SourceSans
Toggle.Text = "ToggleUi"
Toggle.TextColor3 = Color3.fromRGB(248, 248, 248)
Toggle.TextSize = 28.000
Toggle.Draggable = true
Toggle.MouseButton1Click:connect(function()
    Library:ToggleUI()
end)

local Corner = Instance.new("UICorner")
Corner.Name = "Corner"
Corner.Parent = Toggle
end)


local Section = Tab:NewSection("Miscellaneous")
Section:NewButton("Rejoin Game", "Rejoin the current game", function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, player)
end)
Section:NewButton("Server Hop", "Hop to a different server", function()
    local servers = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100")).data
    for _, server in pairs(servers) do
        if server.playing < server.maxPlayers then
            game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, server.id, player)
            break
        end
    end
end)
Section:NewButton("Copy Game Link", "Copy the game link to clipboard", function()
    setclipboard("https://www.roblox.com/games/" .. game.PlaceId)
end)

